package index

import (
	"expense-tracker/frontend/views/layouts"
	"expense-tracker/frontend/views/components"
	"expense-tracker/types"
)

templ GroupDetail(title string, balance types.BalanceResponse, expenseList []types.ExpenseResponseBrief) {
	@layouts.Base() {
		@components.Navbar() {
			@components.GroupDetail(title, balance, expenseList)
		}
	}
}